{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "arrival_date": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "created_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "destination_id": {
      "anyof": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "number": {
      "anyOf": [{"type": "string"}, {"type": "string"}],
      "faker": "company.name"
    },
    "paid": {"type": "boolean"},
    "shipping_date": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "shop_id": {
      "anyof": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "source_id": {
      "anyof": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "updated_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    }
  },
  "required": ["source_id", "destination_id", "number", "shipping_date"],
  "type": "object"
}

