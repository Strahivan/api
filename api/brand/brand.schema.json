{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "description": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "logo": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "name": {
      "anyOf": [{"type": "string"}, {"type": "string"}],
      "faker": "company.name"
    }
  },
  "required": ["name"],
  "type": "object"
}

