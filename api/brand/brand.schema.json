{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["name"],

  "properties": {
    "name": {
      "anyOf": [
        { "type": "string" },
        { "type": "string" }
      ],
      "faker": "company.name"
    },
    "logo": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255,
      "faker": "random.word"
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255,
      "faker": "random.word"
    },
    "active": {
      "type": "boolean"
    }
  }
}

