{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["customer_id", "source_id", "destination_id"],

  "properties": {
    "customer_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "source_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "destination_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "product_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "trip_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "preorder": {
      "type": "boolean"
    },
    "delivery_date": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    },
    "status": {
      "enum": ["pending", "confirmed", "processing", "delivering", "completed", "completed", "failed", "canceled"]
    },
    "delivery": {
      "enum": ["pickup", "post"]
    },
    "total_price": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "base_price": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "service_charge": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "carrier_charge": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "postage": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "count": {
      "type": "number",
      "faker": { "random.number": [{"max": 10, "min": 1}]
      }
    },
    "color": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "color": { "type": "string" },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "edition": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "edition": { "type": "string" },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "size": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "size": { "type": "string" },
            "weight": { "type": "number" },
            "dimensions": {
              "type": "array",
              "items": {
                "type": "number"
              },
              "maxItems": 3,
              "minItems": 2
            },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "instructions": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "maxLength": 1000,
      "faker": "lorem.paragraph"
    },
    "shipping_address": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "maxLength": 1000,
      "faker": "lorem.paragraph"
    },
    "created_at": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    },
    "updated_at": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    }
  }
}
