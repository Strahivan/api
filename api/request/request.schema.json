{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["customer_id", "source_id", "destination_id", "product_id"],

  "properties": {
    "customer_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "source_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "batch_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "destination_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "product_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "trip_id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "null" }
      ],
      "minimum": 1,
      "faker": { "random.number": [{"max": 5, "min": 1}]}
    },
    "preorder": {
      "type": "boolean"
    },
    "urgent": {
      "type": "boolean"
    },
    "delivery_date": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    },
    "status": {
      "enum": ["pending", "confirmed", "processing", "ready", "delivering", "completed", "failed", "canceled"]
    },
    "collection_method": {
      "enum": ["pickup", "post", "courier"]
    },
    "total_price": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "base_price": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "platform_charge": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "postage": {
      "anyOf": [
        { "type": "number" },
        { "type": "null" }
      ],
      "faker": { "random.number": [{"max": 100, "min": 10}]
      }
    },
    "count": {
      "type": "number",
      "faker": { "random.number": [{"max": 10, "min": 1}]
      }
    },
    "currency": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255,
      "faker": "lorem.word"
    },
    "color": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "gallery": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "color": { "type": "string" },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "variation": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "gallery": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "description": { "type": "string" },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "size": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "size": { "type": "string" },
            "weight": { "type": "number" },
            "dimensions": {
              "type": "array",
              "items": {
                "type": "number"
              },
              "maxItems": 3,
              "minItems": 2
            },
            "delta": { "type": "number" }
          }
        },
        { "type": "null" }
      ]
    },
    "instructions": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "maxLength": 1000,
      "faker": "lorem.paragraph"
    },
    "stripe_charge_id": {
      "type": "string"
    },
    "proof": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "string",
            "faker": "image.imageUrl"
          }
        },
        { "type": "null" }
      ]
    },
    "bank_reference": {
      "type": "string"
    },
    "shipping_address": {
      "type": "object",
      "properties": {
        "line_1": {"type": "string"},
        "line_2": {"type": "string"},
        "city": {"type": "string"},
        "zip": {"type": "string"}
      }
    },
    "active": {
      "type": "boolean"
    },
    "created_at": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    },
    "updated_at": {
      "anyOf": [
        { "type": "string" },
        { "type": "null" }
      ],
      "format": "date-time",
      "chance": { "date": [{"string": true}]}
    }
  }
}
