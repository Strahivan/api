{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "bank_reference": {"type": "string"},
    "base_price": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "batch_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "collection_method": {"enum": ["pickup", "post", "courier"]},
    "color": {
      "anyOf": [
        {
          "properties": {
            "color": {"type": "string"},
            "delta": {"type": "number"},
            "gallery": {"items": {"type": "string"}, "type": "array"},
            "name": {"type": "string"}
          },
          "type": "object"
        },
        {"type": "null"}
      ]
    },
    "cost": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "count": {
      "faker": {"random.number": [{"max": 10, "min": 1}]},
      "type": "number"
    },
    "created_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "currency": {
      "faker": "lorem.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "customer_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "delivery_date": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "delta": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "destination_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "ems_fee": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "instructions": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "faker": "lorem.paragraph",
      "maxLength": 1000
    },
    "local_delivery_fee": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "platform_charge": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "postage": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "preorder": {"type": "boolean"},
    "product_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "proof": {
      "anyOf": [
        {
          "items": {"faker": "image.imageUrl", "type": "string"},
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "shipping_address": {
      "properties": {
        "city": {"type": "string"},
        "line_1": {"type": "string"},
        "line_2": {"type": "string"},
        "zip": {"type": "string"}
      },
      "type": "object"
    },
    "size": {
      "anyOf": [
        {
          "properties": {
            "delta": {"type": "number"},
            "dimensions": {
              "items": {"type": "number"},
              "maxItems": 3,
              "minItems": 2,
              "type": "array"
            },
            "size": {"type": "string"},
            "weight": {"type": "number"}
          },
          "type": "object"
        },
        {"type": "null"}
      ]
    },
    "source_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "status": {
      "enum": [
        "canceled",
        "completed",
        "confirmed",
        "delivering",
        "failed",
        "pending",
        "processing",
        "ready"
      ]
    },
    "stripe_charge_id": {"type": "string"},
    "tiers": {
      "anyOf": [
        {
          "items": {
            "properties": {
              "max": {"type": "number"},
              "min": {"type": "number"},
              "rate": {"type": "number"}
            },
            "type": "object"
          },
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "total_price": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "trip_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "updated_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "urgent": {"type": "boolean"},
    "variation": {
      "anyOf": [
        {
          "properties": {
            "delta": {"type": "number"},
            "description": {"type": "string"},
            "gallery": {"items": {"type": "string"}, "type": "array"},
            "name": {"type": "string"}
          },
          "type": "object"
        },
        {"type": "null"}
      ]
    },
    "weight": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    }
  },
  "required": ["customer_id", "source_id", "destination_id", "product_id"],
  "type": "object"
}
