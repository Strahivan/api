{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "currency": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "ems_fee": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "number"
    },
    "lang": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "name": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "shortcode": {
      "faker": "random.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "tiers": {
      "anyOf": [
        {
          "items": {
            "properties": {
              "max": {"type": "number"},
              "min": {"type": "number"},
              "rate": {"type": "number"}
            },
            "type": "object"
          },
          "type": "array"
        },
        {"type": "null"}
      ]
    }
  },
  "required": ["name"],
  "type": "object"
}
