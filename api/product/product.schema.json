{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "brand_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "category_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "colors": {
      "anyOf": [
        {
          "items": {
            "properties": {
              "color": {"type": "string"},
              "delta": {"type": "number"},
              "gallery": {"items": {"type": "string"}, "type": "array"},
              "name": {"type": "string"},
              "out_of_stock": {"type": "boolean"}
            },
            "type": "object"
          },
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "cost": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "discount": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "courier": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "created_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "currency": {
      "faker": "lorem.word",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "delivery_time": {
      "faker": {"random.number": [{"max": 100, "min": 10}]},
      "maxLength": 12,
      "minLength": 1,
      "type": "integer"
    },
    "description": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "faker": "lorem.paragraph",
      "maxLength": 10000
    },
    "dimensions": {
      "items": {"type": "number"},
      "maxItems": 3,
      "minItems": 2,
      "type": "array"
    },
    "featured": {"type": "boolean"},
    "gallery": {
      "anyOf": [
        {
          "items": {"faker": "image.imageUrl", "type": "string"},
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "local_delivery_fee": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "name": {
      "faker": "lorem.sentence",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "order_count": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 200, "min": 1}]},
      "minimum": 1
    },
    "platform_charge": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "postage": {
      "anyOf": [{"type": "number"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "preorder": {"type": "boolean"},
    "price": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "price_override": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    },
    "shop_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "sizes": {
      "anyOf": [
        {
          "items": {
            "properties": {
              "delta": {"type": "number"},
              "dimensions": {
                "items": {"type": "number"},
                "maxItems": 3,
                "minItems": 2,
                "type": "array"
              },
              "name": {"type": "string"},
              "size": {"type": "string"},
              "weight": {"type": "number"},
              "out_of_stock": {"type": "boolean"}
            },
            "type": "object"
          },
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "source_id": {
      "anyOf": [{"type": "integer"}, {"type": "null"}],
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "minimum": 1
    },
    "updated_at": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "chance": {"date": [{"string": true}]},
      "format": "date-time"
    },
    "sold_out": {"type": "boolean"},
    "url": {
      "anyOf": [{"type": "string"}, {"type": "null"}],
      "faker": "internet.url",
      "maxLength": 512
    },
    "variations": {
      "anyOf": [
        {
          "items": {
            "properties": {
              "delta": {"type": "number"},
              "description": {"type": "string"},
              "gallery": {"items": {"type": "string"}, "type": "array"},
              "name": {"type": "string"},
              "out_of_stock": {"type": "boolean"}
            },
            "type": "object"
          },
          "type": "array"
        },
        {"type": "null"}
      ]
    },
    "weight": {
      "anyOf": [{"type": "number"}],
      "faker": {"random.number": [{"max": 100, "min": 10}]}
    }
  },
  "required": ["name"],
  "type": "object"
}

