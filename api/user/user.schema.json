{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {"type": "boolean"},
    "address": {
      "properties": {
        "city": {"type": "string"},
        "line_1": {"type": "string"},
        "line_2": {"type": "string"},
        "zip": {"type": "string"}
      },
      "type": "object"
    },
    "avatar": {"faker": "image.imageUrl", "type": "string"},
    "country_id": {
      "faker": {"random.number": [{"max": 5, "min": 1}]},
      "type": "integer"
    },
    "created_at": {
      "chance": {"date": [{"string": true}]},
      "format": "date-time",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "email": {
      "faker": "internet.email",
      "format": "email",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "hash": {
      "chance": "hash",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "name": {
      "faker": "name.findName",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    },
    "phone": {"type": "string"},
    "role": {"enum": ["admin"]},
    "stripe_token": {"type": "string"},
    "updated_at": {
      "chance": {"date": [{"string": true}]},
      "format": "date-time",
      "maxLength": 255,
      "minLength": 1,
      "type": "string"
    }
  },
  "required": [],
  "type": "object"
}
